    <status>
      <id>9</id>
      <name>Mise a jour du parent</name>
      <colour>#ffffff</colour>
      <visibility>
        <role>__restricted__</role>
      </visibility>
      <items>
        <item type="webservice_call" id="1">
          <label>Mise a jour du parent</label>
          <method>PATCH</method>
          <url>{{ passerelle_url}}passerelle-imio-ia-aes/aes/persons/{{ form_workflow_data_recherche_response_parent_id }}?partner_type=parent</url>
          <post>False</post>
          <post_data>
            <item>
              <name>locality_box</name>
              <value>{{ form_user_var_num_box }}</value>
            </item>
            <item>
              <name>municipality_zipcodes</name>
              <value>{{ commune_cp }}</value>
            </item>
            <item>
              <name>parent_city</name>
              <value>{{ form_user_var_city }}</value>
            </item>
            <item>
              <name>parent_country</name>
              <value>{{ form_user_var_country }}</value>
            </item>
            <item>
              <name>parent_country_id</name>
              <value>{% for country in data_source.aes_countries %}{% if country.text|lower == form_user_var_country|lower %}{{ country.id }}{% endif %}{% endfor %}</value>
            </item>
            <item>
              <name>parent_email</name>
              <value>{{ form_user_email }}</value>
            </item>
            <item>
              <name>parent_locality_id</name>
              <value>{% for locality in data_source.portail_parent_localites %}{% if locality.name|lower == form_user_var_city|lower %}{{ locality.id }}{% endif %}{% endfor %}</value>
            </item>
            <item>
              <name>parent_mobile_phone</name>
              <value>{{ form_workflow_data_recuperation_parent_response_mobile|default:"" }}</value>
            </item>
            <item>
              <name>parent_num_box</name>
              <value>{{ form_user_var_num_box }}</value>
            </item>
            <item>
              <name>parent_num_house</name>
              <value>{{ form_user_var_num_house|default:"" }}</value>
            </item>
            <item>
              <name>parent_phone</name>
              <value>{{ form_user_var_phone }}</value>
            </item>
            <item>
              <name>parent_professional_phone</name>
              <value>{{ form_workflow_data_recuperation_parent_response_phone|default:"" }}</value>
            </item>
            <item>
              <name>parent_street</name>
              <value>{% firstof form_user_var_address form_user_var_street %}</value>
            </item>
            <item>
              <name>parent_zipcode</name>
              <value>{{ form_user_var_zipcode }}</value>
            </item>
          </post_data>
          <response_type>json</response_type>
          <varname>update_parent</varname>
          <store_all_responses>False</store_all_responses>
          <attach_file_to_history>True</attach_file_to_history>
