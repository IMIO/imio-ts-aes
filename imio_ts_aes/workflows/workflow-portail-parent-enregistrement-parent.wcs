        </item>
        <item type="set-backoffice-fields" id="2">
          <label>Donn&#233;es parent AES</label>
          <fields>
            <field>
              <field_id>bob812b7f1-c6b1-4d9a-aa5e-e02093c41ef9</field_id>
              <value>{% block_value email=form_workflow_data_recuperation_parent_response_email locality=form_workflow_data_recuperation_parent_response_locality_id|get:1|split:" - "|get:1|default:form_workflow_data_recuperation_parent_response_city|lower phone=form_workflow_data_recuperation_parent_response_phone street=form_workflow_data_recuperation_parent_response_street street_number=form_workflow_data_recuperation_parent_response_street_number zipcode=form_workflow_data_recuperation_parent_response_locality_id|get:1|split:" - "|get:0|default:form_workflow_data_recuperation_parent_response_zip %}</value>
            </field>
          </fields>
        </item>
        <item type="jump" id="3">
          <status>4</status>
          <condition>
            <type>django</type>
            <value>form_var_donnees_parent_ts|get:"data" == form_var_donnees_parent_aes|get:"data"</value>
          </condition>
          <mode>immediate</mode>
          <set_marker_on_status>False</set_marker_on_status>
        </item>
        <item type="jump" id="4">
          <status>9</status>
          <condition>
            <type>django</type>
            <value>form_var_donnees_parent_ts|get:"data" != form_var_donnees_parent_aes|get:"data"</value>
          </condition>
          <mode>immediate</mode>
